{
  "name": "@dataparty/api",
  "private": false,
  "version": "1.2.11",
  "main": "dist/dataparty.js",
  "frontend": "dist/dataparty-browser.js",
  "backend": "dist/dataparty.js",
  "alias": {
    "events": "last-eventemitter"
  },
  "targets": {
    "frontend": {
      "context": "browser",
      "source": "src/index-browser.js",
      "engines": {
        "browsers": "Chrome 80"
      }
    },
    "backend": {
      "context": "node",
      "source": "src/index.js",
      "optimize": true,
      "includeNodeModules": [
        "websocket",
        "es5-ext/*",
        "es5-iterator",
        "es6-symbol",
        "next-tick",
        "yaeti",
        "utf-8-validate",
        "typedarray-to-buffer",
        "bufferutil",
        "json-schema-to-typescript",
        "cli-color",
        "d",
        "simple-peer",
        "last-eventemitter",
        "@hapi/joi",
        "@hapi/hoek",
        "jsonpath-plus",
        "git-repo-info",
        "debug",
        "wreck",
        "pify"
      ],
      "engines": {
        "node": ">= 12"
      }
    }
  },
  "files": [
    "dist",
    "src/*"
  ],
  "scripts": {
    "test": "npx lab",
    "build": "parcel build --no-scope-hoist",
    "prepare": "npm run build",
    "watch": "parcel watch",
    "build-test": "node ./examples/party/example-build.js",
    "watch-test": "DEBUG=* nodemon --ignore service test/test-service-compile.js",
    "build-docs": "npx jsdoc -c jsdoc.json"
  },
  "dependencies": {
    "@dataparty/bouncer-db": "1.0.1",
    "@dataparty/crypto": "1.0.1",
    "@hapi/joi": "^17.1.1",
    "@zeit/ncc": "^0.22.3",
    "ajv": "6.9.1",
    "body-parser": "^1.19.0",
    "bson": "^4.6.1",
    "bson-objectid": "^1.3.0",
    "colors": "1.3.1",
    "cors": "^2.8.5",
    "debug": "^3.1.0",
    "deep-set": "^1.0.1",
    "dom-storage": "^2.1.0",
    "express": "^4.17.1",
    "express-list-routes": "^0.1.4",
    "git-repo-info": "^2.1.1",
    "jshashes": "^1.0.8",
    "jsonpath-plus": "^0.20.1",
    "last-eventemitter": "^1.1.1",
    "lodash": "^4.17.4",
    "lokijs": "1.5.6",
    "mkdirp": "^0.5.1",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "nconf": "^0.10.0",
    "node-persist": "^3.0.1",
    "nodemon-webpack-plugin": "^3.0.1",
    "origin-router": "^1.6.4",
    "parse-url": "^5.0.1",
    "pify": "^6.1.0",
    "promisfy": "^1.2.0",
    "prompt": "^1.0.0",
    "roslib": "^0.20.0",
    "sanitize-filename": "^1.6.3",
    "simple-peer": "^9.7.2",
    "source-map": "^0.7.3",
    "store-js": "^2.0.4",
    "tingodb": "^0.6.1",
    "touch": "^3.1.0",
    "url-parse": "^1.4.7",
    "uuid": "^3.2.1",
    "uuidv4": "^6.2.12",
    "vm2": "^3.9.2",
    "websocket": "github:sevenbitbyte/WebSocket-Node#parcel-build",
    "wreck": "14.0.2"
  },
  "devDependencies": {
    "@dataparty/bouncer-model": "1.4.3",
    "@hapi/code": "^9.0.1",
    "@hapi/lab": "^25.0.1",
    "better-docs": "^1.1.6",
    "docdash": "^1.1.1",
    "jsdoc": "^3.6.2",
    "minami": "^1.2.3",
    "parcel": "^2.3.1",
    "tmp": "^0.2.1",
    "wrtc": "^0.4.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/datapartyjs/dataparty-api.git"
  },
  "author": "dataparty <code@dataparty.xyz>",
  "license": "Apache-2.0"
}

{
  "name": "@dataparty/api",
  "private": false,
  "version": "1.2.25",
  "main": "dist/dataparty.js",
  "frontend": "dist/dataparty-browser.js",
  "backend": "dist/dataparty.js",
  "embedded": "dist/dataparty-embedded.js",
  "alias": {
    "events": "eventemitter3",
    "process": "process/browser.js",
    "path": "path-browserify",
    "crypto": "crypto-browserify"
  },
  "targets": {
    "frontend": {
      "context": "browser",
      "source": "src/index-browser.js",
      "optimize": true,
      "outputFormat": "global",
      "includeNodeModules": true,
      "engines": {
        "browsers": "Chrome 80"
      }
    },
    "backend": {
      "context": "node",
      "source": "src/index.js",
      "optimize": false,
      "includeNodeModules": false,
      "engines": {
        "node": ">= 12"
      }
    },
    "embedded": {
      "context": "electron-main",
      "source": "src/index-embedded.js",
      "optimize": false,
      "scopeHoist": false,
      "includeNodeModules": true,
      "engines": {
        "node": ">= 12"
      }
    }
  },
  "files": [
    "dist",
    "src/*"
  ],
  "scripts": {
    "test": "npx lab",
    "build": "npx parcel build --no-scope-hoist",
    "prepare": "npm run build",
    "watch": "parcel watch",
    "clean": "rm -rf node_modules; rm package-lock.json; npm i",
    "build-test": "node ./examples/party/example-build.js",
    "watch-test": "DEBUG=* nodemon --ignore service test/test-service-compile.js",
    "build-docs": "./scripts/build-docs.sh",
    "generate-docs": "npx jsdoc --configure jsdoc.json --verbose"
  },
  "dependencies": {
    "@dataparty/bouncer-db": "1.0.1",
    "@dataparty/crypto": "git://github.com/datapartyjs/dataparty-crypto.git",
    "@dataparty/tasker": "^0.0.3",
    "@diva.exchange/i2p-sam": "^5.4.1",
    "@markwylde/liferaft": "^1.3.6",
    "@sevenbitbyte/ncc": "0.0.2",
    "ajv": "8.17.1",
    "axios": "^1.7.7",
    "body-parser": "^1.20.3",
    "bson": "^6.9.0",
    "bson-objectid": "^2.0.4",
    "bufferutil": "^4.0.8",
    "colors": "1.4.0",
    "cors": "^2.8.5",
    "debug": "^4.3.7",
    "dom-storage": "^2.1.0",
    "eventemitter3": "^5.0.1",
    "express": "^4.21.1",
    "express-list-routes": "^1.2.4",
    "git-repo-info": "^2.1.1",
    "joi": "^17.13.3",
    "joi-objectid": "^4.0.2",
    "jshashes": "^1.0.8",
    "jsonpath-plus": "^10.2.0",
    "last-eventemitter": "^1.1.1",
    "lodash": "^4.17.21",
    "lokijs": "1.5.12",
    "mdns": "git://github.com/sevenbitbyte/node_mdns",
    "mkdirp": "^3.0.1",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nconf": "^0.12.1",
    "node-mocks-http": "^1.16.1",
    "node-object-hash": "^3.0.0",
    "node-persist": "^4.0.3",
    "origin-router": "^1.6.4",
    "parse-url": "^9.2.0",
    "promisfy": "^1.2.0",
    "roslib": "^1.4.1",
    "sanitize-filename": "^1.6.3",
    "simple-peer": "9.11.1",
    "source-map": "^0.7.4",
    "store-js": "^2.0.4",
    "tingodb": "^0.6.1",
    "touch": "^3.1.1",
    "url-parse": "^1.5.10",
    "utf-8-validate": "^6.0.5",
    "uuid": "^11.0.3",
    "vm2": "^3.9.19",
    "ws": "^8.18.0",
    "zangodb": "https://github.com/sevenbitbyte/zangodb#hash-patch"
  },
  "devDependencies": {
    "@dataparty/bouncer-model": "1.4.3",
    "@hapi/code": "^9.0.3",
    "@hapi/lab": "^26.0.0",
    "argon2": "^0.41.1",
    "argon2-browser": "^1.18.0",
    "assert": "^2.1.0",
    "browserify-zlib": "^0.2.0",
    "clean-jsdoc-theme": "^4.3.0",
    "crypto-browserify": "^3.12.1",
    "docdash": "^2.0.2",
    "fake-indexeddb": "^6.0.0",
    "https-browserify": "^1.0.0",
    "jsdoc": "^4.0.4",
    "minami": "^1.2.3",
    "parcel": "^2.13.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "prompt": "^1.3.0",
    "punycode": "^2.3.1",
    "querystring-es3": "^0.2.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "string_decoder": "^1.3.0",
    "tmp": "^0.2.3",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "vm-browserify": "^1.1.2",
    "wrtc": "npm:@koush/wrtc@^0.5.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/datapartyjs/dataparty-api.git"
  },
  "author": "dataparty <code@dataparty.xyz>",
  "license": "Apache-2.0"
}
